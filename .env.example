# Auth Provider Selection
# Options: api, firebase, prisma, baas
AUTH_PROVIDER=baas

# Your External Auth API (used when AUTH_PROVIDER=api)
AUTH_API_URL=http://localhost:3001
AUTH_API_KEY=
AUTH_API_TIMEOUT=10000

# ChatAI Web BaaS API Configuration (used when AUTH_PROVIDER=baas)
# For local development with emulators: http://localhost:5001/your-project-id/us-central1/api
# For GCP production: https://us-central1-your-project-id.cloudfunctions.net/api
CHATAI_API_URL=http://localhost:5001/your-project-id/us-central1/api
CHATAI_API_TOKEN=your-api-token-here
CHATAI_API_TIMEOUT=10000

# Firebase Configuration (used when AUTH_PROVIDER=firebase)
FIREBASE_PROJECT_ID=
FIREBASE_CLIENT_EMAIL=
FIREBASE_PRIVATE_KEY=

# Server
PORT=3000
CORS_ORIGIN=http://localhost:5173
